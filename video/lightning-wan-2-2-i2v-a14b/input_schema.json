{
  "$defs": {
    "File": {
      "description": "A class representing a file in the inference.sh ecosystem.",
      "properties": {
        "uri": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Uri"
        },
        "path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Path"
        },
        "content_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Content Type"
        },
        "size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Size"
        },
        "filename": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Filename"
        }
      },
      "title": "File",
      "type": "object"
    }
  },
  "properties": {
    "image": {
      "$ref": "#/$defs/File",
      "description": "Input image for video generation"
    },
    "resolution": {
      "choices": [
        "480p",
        "720p"
      ],
      "default": "480p",
      "description": "Resolution for the output video",
      "enum": [
        "480p",
        "720p"
      ],
      "title": "Resolution",
      "type": "string"
    },
    "end_image": {
      "anyOf": [
        {
          "$ref": "#/$defs/File"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Optional end frame image; when provided, the video will transition from the first frame to this last frame"
    },
    "prompt": {
      "description": "Text prompt for video generation",
      "title": "Prompt",
      "type": "string"
    },
    "negative_prompt": {
      "default": "oversaturated, overexposed, static, blurry details, subtitles, stylized, artwork, painting, still image, overall gray, worst quality, low quality, JPEG artifacts, ugly, deformed, extra fingers, poorly drawn hands, poorly drawn face, malformed, disfigured, deformed limbs, fused fingers, static motionless frame, cluttered background, three legs, crowded background, walking backwards",
      "description": "Negative prompt to guide what to avoid in generation",
      "title": "Negative Prompt",
      "type": "string"
    },
    "num_frames": {
      "default": 81,
      "description": "Number of frames to generate",
      "title": "Num Frames",
      "type": "integer"
    },
    "num_inference_steps": {
      "default": 4,
      "description": "Number of denoising steps (4 or 6 are typically enough for Lightning)",
      "title": "Num Inference Steps",
      "type": "integer"
    },
    "fps": {
      "default": 16,
      "description": "Frames per second for the output video",
      "title": "Fps",
      "type": "integer"
    },
    "seed": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Random seed for reproducibility",
      "title": "Seed"
    },
    "boundary_ratio": {
      "default": 0.875,
      "description": "Boundary ratio between high and low noise transformers (0-1)",
      "maximum": 1.0,
      "minimum": 0.0,
      "title": "Boundary Ratio",
      "type": "number"
    }
  },
  "required": [
    "image",
    "prompt"
  ],
  "title": "AppInput",
  "type": "object"
}