{
  "input": {
    "$defs": {
      "File": {
        "description": "A class representing a file in the inference.sh ecosystem.",
        "properties": {
          "uri": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Uri"
          },
          "path": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Path"
          },
          "content_type": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Content Type"
          },
          "size": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Size"
          },
          "filename": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Filename"
          }
        },
        "title": "File",
        "type": "object"
      }
    },
    "properties": {
      "prompt": {
        "description": "The input prompt describing the video to generate",
        "title": "Prompt",
        "type": "string"
      },
      "image": {
        "$ref": "#/$defs/File",
        "description": "Optional input image for I2V mode"
      },
      "i2v_effect": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "description": "Choose an effect",
        "enum": [
          "Assassin",
          "Baby",
          "Bride",
          "Cakeify",
          "Cartoon_Jaw_Drop",
          "Classy",
          "Crush",
          "Deflate",
          "Disney-Princess",
          "Dolly-Effect",
          "Electrify",
          "Explode",
          "Fus-Ro-Dah",
          "Gun-Shooting",
          "Hug-Jesus",
          "Hulk-Transformation",
          "Inflate",
          "Jumpscare",
          "Jungle",
          "Laughing",
          "Mona-Lisa",
          "Muscle",
          "Painting",
          "Pirate-Captain",
          "Princess",
          "Puppy",
          "Robot-Face-Reveal",
          "Rotate",
          "Samurai",
          "Selfie-With-Younger-Self",
          "Snow-White",
          "Squish",
          "Super-Saiyan",
          "VIP",
          "Warrior",
          "Zen",
          "angry-face",
          "crying",
          "kissing"
        ],
        "title": "I2V Effect"
      },
      "negative_prompt": {
        "default": "",
        "description": "The negative prompt to guide generation",
        "title": "Negative Prompt",
        "type": "string"
      },
      "num_frames": {
        "default": 33,
        "description": "Number of frames to generate",
        "title": "Num Frames",
        "type": "integer"
      },
      "fps": {
        "default": 16,
        "description": "Frames per second for output video",
        "title": "Fps",
        "type": "integer"
      },
      "guidance_scale": {
        "default": 5.0,
        "description": "Guidance scale for I2V generation",
        "title": "Guidance Scale",
        "type": "number"
      }
    },
    "required": [
      "prompt",
      "image",
      "i2v_effect"
    ],
    "title": "AppInput",
    "type": "object"
  },
  "output": {
    "$defs": {
      "File": {
        "description": "A class representing a file in the inference.sh ecosystem.",
        "properties": {
          "uri": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Uri"
          },
          "path": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Path"
          },
          "content_type": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Content Type"
          },
          "size": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Size"
          },
          "filename": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Filename"
          }
        },
        "title": "File",
        "type": "object"
      }
    },
    "properties": {
      "video_output": {
        "$ref": "#/$defs/File",
        "description": "The generated video File"
      }
    },
    "required": [
      "video_output"
    ],
    "title": "AppOutput",
    "type": "object"
  }
}