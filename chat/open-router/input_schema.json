{
  "$defs": {
    "ContextMessage": {
      "properties": {
        "role": {
          "$ref": "#/$defs/ContextMessageRole",
          "description": "the role of the message. user, assistant, or system"
        },
        "text": {
          "description": "the text content of the message",
          "title": "Text",
          "type": "string"
        },
        "reasoning": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "the reasoning content of the message",
          "title": "Reasoning"
        },
        "images": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/File"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "the images of the message",
          "title": "Images"
        },
        "files": {
          "anyOf": [
            {
              "items": {
                "$ref": "#/$defs/File"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "the files of the message",
          "title": "Files"
        },
        "tool_calls": {
          "anyOf": [
            {
              "items": {
                "additionalProperties": true,
                "type": "object"
              },
              "type": "array"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "the tool calls of the message",
          "title": "Tool Calls"
        },
        "tool_call_id": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "the tool call id for tool role messages",
          "title": "Tool Call Id"
        }
      },
      "required": [
        "role",
        "text"
      ],
      "title": "ContextMessage",
      "type": "object"
    },
    "ContextMessageRole": {
      "enum": [
        "user",
        "assistant",
        "system",
        "tool"
      ],
      "title": "ContextMessageRole",
      "type": "string"
    },
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "description": "OpenRouter input model with reasoning and tools support.",
  "properties": {
    "model": {
      "default": "anthropic/claude-3.5-sonnet",
      "description": "The model to use for the OpenRouter API.",
      "enum": [
        "x-ai/grok-code-fast-1",
        "x-ai/grok-4-fast",
        "anthropic/claude-sonnet-4.5",
        "anthropic/claude-sonnet-4",
        "anthropic/claude-3.7-sonnet",
        "anthropic/claude-3.5-sonnet",
        "z-ai/glm-4.6",
        "z-ai/glm-4.5-air:free",
        "z-ai/glm-4.5",
        "z-ai/glm-4.5-air",
        "google/gemini-2.5-pro",
        "google/gemini-2.5-flash",
        "google/gemini-2.0-flash-001",
        "openai/gpt-5",
        "openai/gpt-5-mini",
        "openai/gpt-5-nano",
        "openai/gpt-oss-20b",
        "openai/gpt-4.1-mini",
        "openai/gpt-4o",
        "openai/gpt-4o-mini",
        "qwen/qwen3-vl-30b-a3b-thinking",
        "qwen/qwen3-vl-235b-a22b-instruct",
        "tngtech/deepseek-r1t2-chimera"
      ],
      "title": "OpenRouterModelEnum",
      "type": "string"
    },
    "reasoning_effort": {
      "default": "high",
      "description": "Reasoning effort level: 'high', 'medium', or 'low'",
      "enum": [
        "high",
        "medium",
        "low"
      ],
      "title": "Reasoning Effort",
      "type": "string"
    },
    "reasoning_max_tokens": {
      "default": null,
      "description": "Maximum tokens to allocate for reasoning (overrides effort)",
      "title": "Reasoning Max Tokens",
      "type": "integer"
    },
    "reasoning_exclude": {
      "default": false,
      "description": "Exclude reasoning tokens from response",
      "title": "Reasoning Exclude",
      "type": "boolean"
    },
    "images": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/File"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "the images to use for the model",
      "title": "Images"
    },
    "tools": {
      "anyOf": [
        {
          "items": {
            "additionalProperties": true,
            "type": "object"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "tool definitions for function calling",
      "title": "Tools"
    },
    "tool_call_id": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "the tool call id for tool role messages",
      "title": "Tool Call Id"
    },
    "reasoning": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "the reasoning input of the message",
      "title": "Reasoning"
    },
    "system_prompt": {
      "default": "you are a helpful assistant that can answer questions and help with tasks.",
      "description": "the system prompt to use for the model",
      "examples": [
        "you are a helpful assistant that can answer questions and help with tasks."
      ],
      "title": "System Prompt",
      "type": "string"
    },
    "context": {
      "default": [],
      "description": "the context to use for the model",
      "examples": [
        [
          {
            "content": [
              {
                "text": "What is the capital of France?",
                "type": "text"
              }
            ],
            "role": "user"
          },
          {
            "content": [
              {
                "text": "The capital of France is Paris.",
                "type": "text"
              }
            ],
            "role": "assistant"
          }
        ]
      ],
      "items": {
        "$ref": "#/$defs/ContextMessage"
      },
      "title": "Context",
      "type": "array"
    },
    "role": {
      "$ref": "#/$defs/ContextMessageRole",
      "default": "user",
      "description": "the role of the input text"
    },
    "text": {
      "description": "the input text to use for the model",
      "examples": [
        "write a haiku about artificial general intelligence"
      ],
      "title": "Text",
      "type": "string"
    },
    "temperature": {
      "default": 0.7,
      "maximum": 1.0,
      "minimum": 0.0,
      "title": "Temperature",
      "type": "number"
    },
    "top_p": {
      "default": 0.95,
      "maximum": 1.0,
      "minimum": 0.0,
      "title": "Top P",
      "type": "number"
    },
    "context_size": {
      "default": 4096,
      "title": "Context Size",
      "type": "integer"
    }
  },
  "required": [
    "text"
  ],
  "title": "LLMInput",
  "type": "object"
}