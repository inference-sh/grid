{
  "description": "Input schema for Python code executor.",
  "properties": {
    "code": {
      "description": "Python code to execute",
      "title": "Code",
      "type": "string"
    },
    "timeout": {
      "default": 30,
      "description": "Execution timeout in seconds (1-300)",
      "maximum": 300,
      "minimum": 1,
      "title": "Timeout",
      "type": "integer"
    },
    "capture_output": {
      "default": true,
      "description": "Whether to capture stdout/stderr output",
      "title": "Capture Output",
      "type": "boolean"
    },
    "working_dir": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Optional working directory for code execution",
      "title": "Working Dir"
    }
  },
  "required": [
    "code"
  ],
  "title": "AppInput",
  "type": "object"
}