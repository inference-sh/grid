{
  "$defs": {
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    },
    "GeometryFormatEnum": {
      "description": "Output geometry file format options.",
      "enum": [
        "glb",
        "usdz",
        "fbx",
        "obj",
        "stl"
      ],
      "title": "GeometryFormatEnum",
      "type": "string"
    },
    "MaterialEnum": {
      "description": "Material type options.",
      "enum": [
        "PBR",
        "Shaded",
        "All"
      ],
      "title": "MaterialEnum",
      "type": "string"
    },
    "QualityMeshEnum": {
      "description": "Mesh quality and resolution options.",
      "enum": [
        "500K Triangle",
        "200K Triangle",
        "100K Triangle",
        "4K Quad",
        "2K Quad",
        "1K Quad"
      ],
      "title": "QualityMeshEnum",
      "type": "string"
    }
  },
  "properties": {
    "prompt": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Text guidance for 3D model generation (e.g., 'A futuristic robot with sleek metallic design')",
      "title": "Prompt"
    },
    "input_images": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/File"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Up to 5 reference images for 3D generation (optional if prompt is provided)",
      "title": "Input Images"
    },
    "geometry_file_format": {
      "$ref": "#/$defs/GeometryFormatEnum",
      "default": "glb",
      "description": "Output format for the 3D model geometry"
    },
    "material": {
      "$ref": "#/$defs/MaterialEnum",
      "default": "All",
      "description": "Material type for the 3D model"
    },
    "quality_mesh_option": {
      "$ref": "#/$defs/QualityMeshEnum",
      "default": "500K Triangle",
      "description": "Mesh resolution and type"
    },
    "use_original_alpha": {
      "default": false,
      "description": "Use original alpha channel from input images",
      "title": "Use Original Alpha",
      "type": "boolean"
    },
    "seed": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Random seed for reproducible generation",
      "title": "Seed"
    },
    "preview_render": {
      "default": false,
      "description": "Generate preview renders of the 3D model",
      "title": "Preview Render",
      "type": "boolean"
    }
  },
  "title": "AppInput",
  "type": "object"
}