{
  "$defs": {
    "LoraConfig": {
      "properties": {
        "adapter_name": {
          "description": "Name for the LoRA adapter.",
          "title": "Adapter Name",
          "type": "string"
        },
        "lora_url": {
          "description": "URL to LoRA file (.safetensors) or Civitai model page",
          "title": "Lora Url",
          "type": "string"
        },
        "lora_multiplier": {
          "default": 1.0,
          "description": "Multiplier for the LoRA effect",
          "title": "Lora Multiplier",
          "type": "number"
        }
      },
      "required": [
        "adapter_name",
        "lora_url"
      ],
      "title": "LoraConfig",
      "type": "object"
    }
  },
  "properties": {
    "prompt": {
      "description": "The text prompt to generate an image from.",
      "title": "Prompt",
      "type": "string"
    },
    "height": {
      "default": 1024,
      "description": "The height in pixels of the generated image.",
      "title": "Height",
      "type": "integer"
    },
    "width": {
      "default": 1024,
      "description": "The width in pixels of the generated image.",
      "title": "Width",
      "type": "integer"
    },
    "num_inference_steps": {
      "default": 30,
      "description": "The number of inference steps.",
      "title": "Num Inference Steps",
      "type": "integer"
    },
    "guidance_scale": {
      "default": 3.5,
      "description": "The guidance scale.",
      "title": "Guidance Scale",
      "type": "number"
    },
    "seed": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "The seed for random generation.",
      "title": "Seed"
    },
    "loras": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/LoraConfig"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "List of LoRA configs to apply",
      "title": "Loras"
    },
    "denoise": {
      "default": 1.0,
      "description": "Denoising strength (0.0 to 1.0, where 1.0 is standard full denoising)",
      "maximum": 1.0,
      "minimum": 0.0,
      "title": "Denoise",
      "type": "number"
    },
    "negative_prompt": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Negative prompt to steer away from undesired outputs.",
      "title": "Negative Prompt"
    }
  },
  "required": [
    "prompt"
  ],
  "title": "AppInput",
  "type": "object"
}