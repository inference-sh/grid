{
  "input": {
    "$defs": {
      "File": {
        "description": "A class representing a file in the inference.sh ecosystem.",
        "properties": {
          "uri": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Uri"
          },
          "path": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Path"
          },
          "content_type": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Content Type"
          },
          "size": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Size"
          },
          "filename": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Filename"
          }
        },
        "title": "File",
        "type": "object"
      },
      "SeamFixMode": {
        "enum": [
          "none",
          "band_pass",
          "half_tile",
          "half_tile_plus_intersections"
        ],
        "title": "SeamFixMode",
        "type": "string"
      },
      "UpscaleMode": {
        "enum": [
          "linear",
          "chess",
          "none"
        ],
        "title": "UpscaleMode",
        "type": "string"
      }
    },
    "properties": {
      "image": {
        "$ref": "#/$defs/File",
        "description": "The image to upscale"
      },
      "target_width": {
        "default": 2048,
        "description": "The width of the output image",
        "examples": [
          1024,
          2048,
          4096
        ],
        "maximum": 4096,
        "minimum": 512,
        "title": "Target Width",
        "type": "integer"
      },
      "target_height": {
        "default": 2048,
        "description": "The height of the output image",
        "examples": [
          1024,
          2048,
          4096
        ],
        "maximum": 4096,
        "minimum": 512,
        "title": "Target Height",
        "type": "integer"
      },
      "tile_width": {
        "default": 1024,
        "description": "The width of the tile to upscale",
        "examples": [
          512,
          1024,
          2048
        ],
        "maximum": 2048,
        "minimum": 256,
        "title": "Tile Width",
        "type": "integer"
      },
      "tile_height": {
        "default": 1024,
        "description": "The height of the tile to upscale",
        "examples": [
          512,
          1024,
          2048
        ],
        "maximum": 2048,
        "minimum": 256,
        "title": "Tile Height",
        "type": "integer"
      },
      "redraw_padding": {
        "default": 32,
        "description": "The padding to redraw",
        "examples": [
          16,
          32,
          64
        ],
        "maximum": 128,
        "minimum": 0,
        "title": "Redraw Padding",
        "type": "integer"
      },
      "redraw_mask_blur": {
        "default": 8,
        "description": "The blur radius for the redraw mask",
        "examples": [
          4,
          8,
          16
        ],
        "maximum": 32,
        "minimum": 0,
        "title": "Redraw Mask Blur",
        "type": "integer"
      },
      "upscale_mode": {
        "$ref": "#/$defs/UpscaleMode",
        "default": "chess",
        "description": "The mode to upscale the image"
      },
      "seam_fix_mode": {
        "$ref": "#/$defs/SeamFixMode",
        "default": "none",
        "description": "The mode to fix the seams"
      },
      "prompt": {
        "default": "",
        "description": "The prompt for the image",
        "examples": [
          "enhance details, high quality",
          "sharp, clear, detailed"
        ],
        "title": "Prompt",
        "type": "string"
      },
      "negative_prompt": {
        "default": "",
        "description": "The negative prompt for the image",
        "examples": [
          "blurry, low quality",
          "noise, artifacts"
        ],
        "title": "Negative Prompt",
        "type": "string"
      },
      "strength": {
        "default": 0.3,
        "description": "The strength of the prompt",
        "examples": [
          0.3,
          0.5,
          0.7
        ],
        "maximum": 1.0,
        "minimum": 0.0,
        "title": "Strength",
        "type": "number"
      },
      "guidance_scale": {
        "default": 7.5,
        "description": "The guidance scale for the image",
        "examples": [
          5.0,
          7.5,
          10.0
        ],
        "maximum": 20.0,
        "minimum": 1.0,
        "title": "Guidance Scale",
        "type": "number"
      },
      "seed": {
        "default": 0,
        "description": "The seed for the image",
        "examples": [
          42,
          123456
        ],
        "minimum": 0,
        "title": "Seed",
        "type": "integer"
      }
    },
    "required": [
      "image"
    ],
    "title": "AppInput",
    "type": "object"
  },
  "output": {
    "$defs": {
      "File": {
        "description": "A class representing a file in the inference.sh ecosystem.",
        "properties": {
          "uri": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Uri"
          },
          "path": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Path"
          },
          "content_type": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Content Type"
          },
          "size": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Size"
          },
          "filename": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Filename"
          }
        },
        "title": "File",
        "type": "object"
      }
    },
    "properties": {
      "result": {
        "$ref": "#/$defs/File",
        "description": "The upscaled image"
      }
    },
    "required": [
      "result"
    ],
    "title": "AppOutput",
    "type": "object"
  }
}