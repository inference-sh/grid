{
  "$defs": {
    "File": {
      "description": "A class representing a file in the inference.sh ecosystem.",
      "properties": {
        "uri": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Uri"
        },
        "path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Path"
        },
        "content_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Content Type"
        },
        "size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Size"
        },
        "filename": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Filename"
        }
      },
      "title": "File",
      "type": "object"
    }
  },
  "properties": {
    "person_image": {
      "$ref": "#/$defs/File",
      "description": "Input image of the person"
    },
    "object_image": {
      "$ref": "#/$defs/File",
      "description": "Input image of the object to try on"
    },
    "object_class": {
      "description": "Type of object being tried on",
      "enum": [
        "top clothes",
        "bottom clothes",
        "dress",
        "shoe",
        "earrings",
        "bracelet",
        "necklace",
        "ring",
        "sunglasses",
        "glasses",
        "belt",
        "bag",
        "hat",
        "tie",
        "bow tie"
      ],
      "title": "Object Class",
      "type": "string"
    },
    "steps": {
      "default": 20,
      "description": "Number of inference steps",
      "maximum": 50,
      "minimum": 1,
      "title": "Steps",
      "type": "integer"
    },
    "guidance_scale": {
      "default": 30.0,
      "description": "Guidance scale for generation",
      "maximum": 50.0,
      "minimum": 1.0,
      "title": "Guidance Scale",
      "type": "number"
    },
    "seed": {
      "default": -1,
      "description": "Random seed (-1 for random)",
      "title": "Seed",
      "type": "integer"
    }
  },
  "required": [
    "person_image",
    "object_image",
    "object_class"
  ],
  "title": "AppInput",
  "type": "object"
}