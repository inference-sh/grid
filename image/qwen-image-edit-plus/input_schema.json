{
  "$defs": {
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "properties": {
    "images": {
      "description": "Input images (1-3 images) for editing",
      "items": {
        "$ref": "#/$defs/File"
      },
      "title": "Images",
      "type": "array"
    },
    "prompt": {
      "description": "Text prompt describing the desired edit",
      "title": "Prompt",
      "type": "string"
    },
    "negative_prompt": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": " ",
      "description": "Negative prompt to guide what to avoid",
      "title": "Negative Prompt"
    },
    "width": {
      "anyOf": [
        {
          "maximum": 2048,
          "minimum": 256,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Output image width in pixels (defaults to first input image width if not specified)",
      "title": "Width"
    },
    "height": {
      "anyOf": [
        {
          "maximum": 2048,
          "minimum": 256,
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Output image height in pixels (defaults to first input image height if not specified)",
      "title": "Height"
    },
    "num_inference_steps": {
      "default": 30,
      "description": "Number of denoising steps (30 recommended for quality/speed balance)",
      "maximum": 100,
      "minimum": 1,
      "title": "Num Inference Steps",
      "type": "integer"
    },
    "guidance_scale": {
      "default": 1.0,
      "description": "Guidance scale for generation",
      "maximum": 10.0,
      "minimum": 0.0,
      "title": "Guidance Scale",
      "type": "number"
    },
    "true_cfg_scale": {
      "default": 1.0,
      "description": "True CFG scale for the model",
      "maximum": 10.0,
      "minimum": 0.0,
      "title": "True Cfg Scale",
      "type": "number"
    },
    "seed": {
      "default": 42,
      "description": "Random seed for reproducibility",
      "title": "Seed",
      "type": "integer"
    }
  },
  "required": [
    "images",
    "prompt"
  ],
  "title": "AppInput",
  "type": "object"
}