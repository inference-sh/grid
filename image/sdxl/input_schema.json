{
  "$defs": {
    "LoraConfig": {
      "properties": {
        "adapter_name": {
          "description": "Name for the LoRA adapter.",
          "title": "Adapter Name",
          "type": "string"
        },
        "lora_url": {
          "description": "URL to LoRA file (.safetensors) or Civitai model page",
          "title": "Lora Url",
          "type": "string"
        },
        "lora_multiplier": {
          "default": 1.0,
          "description": "Multiplier for the LoRA effect",
          "title": "Lora Multiplier",
          "type": "number"
        }
      },
      "required": [
        "adapter_name",
        "lora_url"
      ],
      "title": "LoraConfig",
      "type": "object"
    }
  },
  "properties": {
    "prompt": {
      "description": "The text prompt to generate the image from",
      "examples": [
        "A majestic lion jumping from a big stone at night"
      ],
      "title": "Prompt",
      "type": "string"
    },
    "negative_prompt": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Negative prompt to avoid certain elements in the generated image",
      "examples": [
        "blurry, low quality, distorted"
      ],
      "title": "Negative Prompt"
    },
    "num_inference_steps": {
      "default": 50,
      "description": "Number of denoising steps",
      "maximum": 100,
      "minimum": 1,
      "title": "Num Inference Steps",
      "type": "integer"
    },
    "guidance_scale": {
      "default": 7.5,
      "description": "Classifier-free guidance scale",
      "maximum": 20.0,
      "minimum": 1.0,
      "title": "Guidance Scale",
      "type": "number"
    },
    "width": {
      "default": 1024,
      "description": "Width of the generated image",
      "maximum": 2048,
      "minimum": 256,
      "title": "Width",
      "type": "integer"
    },
    "height": {
      "default": 1024,
      "description": "Height of the generated image",
      "maximum": 2048,
      "minimum": 256,
      "title": "Height",
      "type": "integer"
    },
    "model_url": {
      "default": "stabilityai/stable-diffusion-xl-base-1.0",
      "description": "URL or huggingface path to a custom Stable Diffusion XL model",
      "examples": [
        "stabilityai/stable-diffusion-xl-base-1.0"
      ],
      "title": "Model Url",
      "type": "string"
    },
    "loras": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/LoraConfig"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "List of LoRA configs to apply",
      "title": "Loras"
    }
  },
  "required": [
    "prompt"
  ],
  "title": "AppInput",
  "type": "object"
}