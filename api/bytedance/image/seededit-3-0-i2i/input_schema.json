{
  "$defs": {
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    }
  },
  "description": "Input schema for SeedEdit 3.0 image editing.",
  "properties": {
    "prompt": {
      "description": "Text prompt describing the desired edit or transformation. Be specific about what changes you want.",
      "examples": [
        "Change the background to a sunset beach",
        "Make the person wear a red dress"
      ],
      "title": "Prompt",
      "type": "string"
    },
    "image": {
      "$ref": "#/$defs/File",
      "description": "Source image to edit. Required for image-to-image editing."
    },
    "guidance_scale": {
      "default": 5.5,
      "description": "Guidance scale for edit strength (1.0-20.0). Higher values follow the prompt more strictly.",
      "maximum": 20.0,
      "minimum": 1.0,
      "title": "Guidance Scale",
      "type": "number"
    },
    "seed": {
      "anyOf": [
        {
          "type": "integer"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Random seed for reproducible results. If not provided, a random seed is used.",
      "title": "Seed"
    },
    "watermark": {
      "default": false,
      "description": "Whether to add a watermark to the generated image.",
      "title": "Watermark",
      "type": "boolean"
    }
  },
  "required": [
    "prompt",
    "image"
  ],
  "title": "AppInput",
  "type": "object"
}