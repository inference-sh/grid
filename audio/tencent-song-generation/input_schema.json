{
  "$defs": {
    "File": {
      "description": "A class representing a file in the inference.sh ecosystem.",
      "properties": {
        "uri": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Uri"
        },
        "path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Path"
        },
        "content_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Content Type"
        },
        "size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Size"
        },
        "filename": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Filename"
        }
      },
      "title": "File",
      "type": "object"
    }
  },
  "properties": {
    "lyrics": {
      "description": "The lyrics for the song to generate",
      "title": "Lyrics",
      "type": "string"
    },
    "descriptions": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Text description of the song style, genre, instruments, etc.",
      "title": "Descriptions"
    },
    "prompt_audio": {
      "anyOf": [
        {
          "$ref": "#/$defs/File"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Optional audio file to use as a prompt/reference"
    },
    "auto_prompt_type": {
      "default": "Auto",
      "description": "Auto prompt type for style selection",
      "enum": [
        "Pop",
        "R&B",
        "Dance",
        "Jazz",
        "Folk",
        "Rock",
        "Chinese Style",
        "Chinese Tradition",
        "Metal",
        "Reggae",
        "Chinese Opera",
        "Auto"
      ],
      "title": "Auto Prompt Type",
      "type": "string"
    },
    "generate_type": {
      "default": "mixed",
      "description": "Type of generation",
      "enum": [
        "vocal",
        "bgm",
        "separate",
        "mixed"
      ],
      "title": "Generate Type",
      "type": "string"
    },
    "temperature": {
      "default": 0.9,
      "description": "Generation temperature",
      "title": "Temperature",
      "type": "number"
    },
    "cfg_coef": {
      "default": 1.5,
      "description": "CFG coefficient",
      "title": "Cfg Coef",
      "type": "number"
    },
    "top_k": {
      "default": 50,
      "description": "Top-k sampling parameter",
      "title": "Top K",
      "type": "integer"
    },
    "top_p": {
      "default": 0.0,
      "description": "Top-p sampling parameter",
      "title": "Top P",
      "type": "number"
    },
    "max_duration": {
      "default": 30,
      "description": "Maximum duration of generated song in seconds",
      "title": "Max Duration",
      "type": "integer"
    }
  },
  "required": [
    "lyrics"
  ],
  "title": "AppInput",
  "type": "object"
}