    "properties": {
      "audio_output": {
        "$ref": "#/$defs/File",
        "description": "Generated audio file"
      },
      "video_output": {
        "anyOf": [
          {
            "$ref": "#/$defs/File"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "description": "Generated video file with audio"
      }
    },
