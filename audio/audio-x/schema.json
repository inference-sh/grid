{
  "input": {
    "$defs": {
      "File": {
        "description": "A class representing a file in the inference.sh ecosystem.",
        "properties": {
          "uri": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Uri"
          },
          "path": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Path"
          },
          "content_type": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Content Type"
          },
          "size": {
            "anyOf": [
              {
                "type": "integer"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Size"
          },
          "filename": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "null"
              }
            ],
            "default": null,
            "title": "Filename"
          }
        },
        "title": "File",
        "type": "object"
      }
    },
    "properties": {
      "prompt": {
        "description": "The text prompt for audio generation",
        "title": "Prompt",
        "type": "string"
      },
      "negative_prompt": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "description": "Negative prompt for generation",
        "title": "Negative Prompt"
      },
      "video_file": {
        "anyOf": [
          {
            "$ref": "#/$defs/File"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "description": "Input video file for conditioning"
      },
      "audio_prompt_file": {
        "anyOf": [
          {
            "$ref": "#/$defs/File"
          },
          {
            "type": "null"
          }
        ],
        "default": null,
        "description": "Input audio file for conditioning"
      },
      "seconds_start": {
        "default": 0,
        "description": "Start time in seconds",
        "title": "Seconds Start",
        "type": "integer"
      },
      "seconds_total": {
        "default": 10,
        "description": "Total duration in seconds",
        "title": "Seconds Total",
        "type": "integer"
      },
      "cfg_scale": {
        "default": 7.0,
        "description": "Classifier-free guidance scale",
        "title": "Cfg Scale",
        "type": "number"
      },
      "steps": {
        "default": 100,
        "description": "Number of diffusion steps",
        "title": "Steps",
        "type": "integer"
      },
      "seed": {
        "default": -1,
        "description": "Random seed (-1 for random)",
        "title": "Seed",
        "type": "integer"
      },
      "sampler_type": {
        "default": "dpmpp-3m-sde",
        "description": "Sampler type",
        "title": "Sampler Type",
        "type": "string"
      },
      "sigma_min": {
        "default": 0.03,
        "description": "Minimum sigma value",
        "title": "Sigma Min",
        "type": "number"
      },
      "sigma_max": {
        "default": 500,
        "description": "Maximum sigma value",
        "title": "Sigma Max",
        "type": "number"
      },
      "cfg_rescale": {
        "default": 0.0,
        "description": "CFG rescale amount",
        "title": "Cfg Rescale",
        "type": "number"
      }
    },
    "required": [
      "prompt"
    ],
    "title": "AppInput",
    "type": "object"
  },
  "output": {
    "$defs": {
      "File": {
        "description": "A class representing a file in the inference.sh ecosystem.",
