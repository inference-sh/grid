{
  "$defs": {
    "File": {
      "description": "A class representing a file in the inference.sh ecosystem.",
      "properties": {
        "uri": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Uri"
        },
        "path": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Path"
        },
        "content_type": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Content Type"
        },
        "size": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Size"
        },
        "filename": {
          "anyOf": [
            {
              "type": "string"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "title": "Filename"
        }
      },
      "title": "File",
      "type": "object"
    }
  },
  "properties": {
    "prompt": {
      "description": "Text prompt for audio generation",
      "title": "Prompt",
      "type": "string"
    },
    "negative_prompt": {
      "default": "",
      "description": "Negative prompt to avoid certain audio characteristics",
      "title": "Negative Prompt",
      "type": "string"
    },
    "video_input": {
      "anyOf": [
        {
          "$ref": "#/$defs/File"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Optional video file to generate synchronized audio for"
    },
    "duration": {
      "default": 8.0,
      "description": "Duration of audio to generate in seconds",
      "title": "Duration",
      "type": "number"
    },
    "cfg_strength": {
      "default": 4.5,
      "description": "Classifier-free guidance strength",
      "title": "Cfg Strength",
      "type": "number"
    },
    "num_steps": {
      "default": 25,
      "description": "Number of diffusion steps",
      "title": "Num Steps",
      "type": "integer"
    },
    "seed": {
      "default": 42,
      "description": "Random seed for reproducible generation",
      "title": "Seed",
      "type": "integer"
    },
    "mask_away_clip": {
      "default": false,
      "description": "Whether to mask away CLIP features",
      "title": "Mask Away Clip",
      "type": "boolean"
    }
  },
  "required": [
    "prompt"
  ],
  "title": "AppInput",
  "type": "object"
}