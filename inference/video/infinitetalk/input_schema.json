      "description": "Audio file for speaker 2"
    },
    "tts_text": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Text for TTS generation",
      "title": "Tts Text"
    },
    "resolution": {
      "default": "infinitetalk-480",
      "description": "Output resolution",
      "enum": [
        "infinitetalk-480",
        "infinitetalk-720"
