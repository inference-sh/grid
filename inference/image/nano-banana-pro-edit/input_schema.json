{
  "$defs": {
    "AspectRatioEnum": {
      "description": "Aspect ratio options.",
      "enum": [
        "auto",
        "21:9",
        "16:9",
        "3:2",
        "4:3",
        "5:4",
        "1:1",
        "4:5",
        "3:4",
        "2:3",
        "9:16",
        "9:21"
      ],
      "title": "AspectRatioEnum",
      "type": "string"
    },
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    },
    "ResolutionEnum": {
      "description": "Resolution options.",
      "enum": [
        "1K",
        "2K",
        "4K"
      ],
      "title": "ResolutionEnum",
      "type": "string"
    }
  },
  "properties": {
    "prompt": {
      "description": "Editing instruction for the image. Example: 'make a photo of the man driving the car down the california coastline'",
      "title": "Prompt",
      "type": "string"
    },
    "images": {
      "description": "The reference image to edit. Supported formats: PNG, JPEG, WebP, AVIF, and HEIF.",
      "items": {
        "$ref": "#/$defs/File"
      },
      "title": "Images",
      "type": "array"
    },
    "aspect_ratio": {
      "$ref": "#/$defs/AspectRatioEnum",
      "default": "auto",
      "description": "Aspect ratio for the output image. Default: auto"
    },
    "resolution": {
      "$ref": "#/$defs/ResolutionEnum",
      "default": "1K",
      "description": "Output resolution. Default: 1K"
    }
  },
  "required": [
    "prompt",
    "images"
  ],
  "title": "AppInput",
  "type": "object"
}