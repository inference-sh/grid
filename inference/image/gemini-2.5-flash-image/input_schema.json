{
  "$defs": {
    "AspectRatioEnum": {
      "description": "Aspect ratio options for generated images.",
      "enum": [
        "21:9",
        "1:1",
        "4:3",
        "3:2",
        "2:3",
        "5:4",
        "4:5",
        "3:4",
        "16:9",
        "9:16"
      ],
      "title": "AspectRatioEnum",
      "type": "string"
    },
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    },
    "OutputFormatEnum": {
      "description": "Output format options.",
      "enum": [
        "jpeg",
        "png"
      ],
      "title": "OutputFormatEnum",
      "type": "string"
    }
  },
  "properties": {
    "prompt": {
      "description": "The prompt for image generation or editing. Describe what you want to create or change.",
      "title": "Prompt",
      "type": "string"
    },
    "images": {
      "anyOf": [
        {
          "items": {
            "$ref": "#/$defs/File"
          },
          "type": "array"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Optional list of input images for editing. When provided, the model will edit these images based on the prompt. When not provided, the model will generate new images from the text prompt. Supported formats: JPEG, PNG, WebP",
      "title": "Images"
    },
    "num_images": {
      "default": 1,
      "description": "Number of images to generate.",
      "maximum": 4,
      "minimum": 1,
      "title": "Num Images",
      "type": "integer"
    },
    "output_format": {
      "$ref": "#/$defs/OutputFormatEnum",
      "default": "png",
      "description": "Output format for the generated images."
    },
    "aspect_ratio": {
      "anyOf": [
        {
          "$ref": "#/$defs/AspectRatioEnum"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Aspect ratio for generated images. Default is 1:1 for generation, or matches input image aspect ratio for editing."
    }
  },
  "required": [
    "prompt"
  ],
  "title": "AppInput",
  "type": "object"
}