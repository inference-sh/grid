{
  "$defs": {
    "AspectRatioEnum": {
      "description": "Aspect ratio options for generated images.",
      "enum": [
        "21:9",
        "1:1",
        "4:3",
        "3:2",
        "2:3",
        "5:4",
        "4:5",
        "3:4",
        "16:9",
        "9:16"
      ],
      "title": "AspectRatioEnum",
      "type": "string"
    },
    "File": {
      "$id": "/schemas/File",
      "oneOf": [
        {
          "description": "A class representing a file in the inference.sh ecosystem.",
          "properties": {
            "uri": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Uri"
            },
            "path": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Path"
            },
            "content_type": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Content Type"
            },
            "size": {
              "anyOf": [
                {
                  "type": "integer"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Size"
            },
            "filename": {
              "anyOf": [
                {
                  "type": "string"
                },
                {
                  "type": "null"
                }
              ],
              "default": null,
              "title": "Filename"
            }
          },
          "title": "File",
          "type": "object"
        },
        {
          "type": "string"
        }
      ]
    },
    "OutputFormatEnum": {
      "description": "Output format options.",
      "enum": [
        "jpeg",
        "png"
      ],
      "title": "OutputFormatEnum",
      "type": "string"
    }
  },
  "properties": {
    "prompt": {
      "description": "The prompt for image editing. Describe what you want to change or create in the image.",
      "title": "Prompt",
      "type": "string"
    },
    "images": {
      "description": "List of input images for editing. Supported formats: JPEG, PNG, WebP",
      "items": {
        "$ref": "#/$defs/File"
      },
      "minItems": 1,
      "title": "Images",
      "type": "array"
    },
    "num_images": {
      "default": 1,
      "description": "Number of images to generate.",
      "maximum": 4,
      "minimum": 1,
      "title": "Num Images",
      "type": "integer"
    },
    "output_format": {
      "$ref": "#/$defs/OutputFormatEnum",
      "default": "jpeg",
      "description": "Output format for the generated images."
    },
    "sync_mode": {
      "default": false,
      "description": "When true, images will be returned as data URIs instead of URLs.",
      "title": "Sync Mode",
      "type": "boolean"
    },
    "aspect_ratio": {
      "anyOf": [
        {
          "$ref": "#/$defs/AspectRatioEnum"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "description": "Aspect ratio for generated images. Default is None, which takes one of the input images' aspect ratio."
    }
  },
  "required": [
    "prompt",
    "images"
  ],
  "title": "AppInput",
  "type": "object"
}